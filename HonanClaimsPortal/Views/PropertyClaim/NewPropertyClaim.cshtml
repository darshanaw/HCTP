<!-- Loading gif-->
<script src="~/Scripts/loadingoverlay.min.js"></script>
<script src="~/Scripts/loadingoverlay_progress.min.js"></script>

<script>

    $(document).ready(function () {

        $('#Property_Suburb').select2({ width: '100%' });

        if ($("#Property_Suburb").length > 0) {
            $("#Property_Suburb").val($("#Property_Suburb").val()).trigger('change.select2');;
        }

        $('#formNewPropertyClaim').submit(function () {
            if ($(this).valid()) {
                $.LoadingOverlay("show");
            }
        });

        jQuery.validator.methods["date"] = function (value, element) { return true; }


        // Warning
        $(window).on('beforeunload', function () {
            return "Any changes will be lost";
        });

        // Form Submit
        $(document).on("submit", "form", function (event) {
            // disable unload warning
            $(window).off('beforeunload');
        });

        $("#btnPolicyLookup").click(function () {

            $('#tbPolicy').DataTable().ajax.reload(null, false);

        });

        $('#dtpReportedDate').on('dp.change', function (e) {
            var today = new Date();
            if (e.date._d > today) {
                $('#dtpReportedDate').data("DateTimePicker").date(new Date())
                bootbox.alert({
                    message: "Reported Date cannot be a future date",
                    size: 'small'
                });
            }
        });

        $('#dtpIncidentDate').on('dp.change', function (e) {
            var today = new Date();
            if (e.date._d > today) {
                $('#dtpIncidentDate').data("DateTimePicker").date(new Date())
                bootbox.alert({
                    message: "Incident Date cannot be a future date",
                    size: 'small'
                });
            }
        });

        $('#dtpDob').on('dp.change', function (e) {
            var today = new Date();
            if (e.date._d > today) {
                $('#dtpDob').data("DateTimePicker").date(new Date())
                bootbox.alert({
                    message: "DoB cannot be a future date",
                    size: 'small'
                });
            }
        });

    });

 function Cancel() {

        $(window).off('beforeunload');
        window.location.href = '/claimlist/Index';
    }

    var firstOpen = true;
    $(function () {
        $('#dtpDob').datetimepicker({
            viewMode: 'years',
            format: 'DD/MM/YYYY',
        }).on("dp.show", function () {
            console.log("dtp open");
            if (firstOpen == true) {
                var year = new Date().getFullYear() - 18;
                $(this).data('DateTimePicker').date("01/01/" + year);
                firstOpen = false;
            }
        });
    });

</script>