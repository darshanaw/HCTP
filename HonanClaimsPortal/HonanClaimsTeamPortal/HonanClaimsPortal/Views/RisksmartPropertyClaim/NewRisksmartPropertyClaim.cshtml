<script>

    $(document).ready(function () {

        $('#Incident_Category').select2();

        //$("#Region").select2({

        //    //multiple: true,
        //    tags: true
        //});

        $("#brokerAccountNameModal").click(function () {
            $('#tbContactBroker').DataTable().ajax.reload(null, false);
        })

        $("#btnPolicyLookup").click(function () {

           $('#tbPolicy').DataTable().ajax.reload(null, false);

        });


        jQuery.validator.methods["date"] = function (value, element) { return true; }

        // Warning
        $(window).on('beforeunload', function () {
            return "Any changes will be lost";
        });

        // Form Submit
        $(document).on("submit", "form", function (event) {
            // disable unload warning
            $(window).off('beforeunload');
        });

    });

    function Cancel() {

        $(window).off('beforeunload');

        if ("@Model.Claim_Type" === "Claim")
            window.location.href = '/Claim/ViewClaims';
        else
            window.location.href = '/Notification/ViewNotifications';
    }

</script>